{
  "type":"object",
  "properties": {
      "licenseCaps":{
          "type":"array",
          "items":{
              "type":"string",
              "pattern":"^((BASE)|(MALWARE)|(THREAT)|(URLFilter)|(PROTECT)|(VPN)|(CONTROL))$"
          }
      },
      "fmcIpforDeviceReg":{
          "type":"string",
          "pattern":"^((DONTRESOLVE)|((?:(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9][0-9]|[0-9])(\\.(?!$)|$)){4}))$"
      },
      "RegistrationId":{
          "type":"string",
          "pattern":"^...*$"
      },
      "NatId":{
          "type":"string",
          "pattern":"^...*$"
      },
      "fmcAccessPolicyName":{
          "type":"string",
          "pattern":"^...*$"
      },
      "fmcPlatformPolicyName":{
          "type":"string",
          "pattern":"^...*$"
      },
      "fmcInsideNicName":{
          "type":"string",
          "pattern":"^...*$"
      },
      "fmcOutsideNicName":{
          "type":"string",
          "pattern":"^...*$"
      },
      "fmcInsideNic":{
          "type":"string",
          "pattern":"^.*0/(0|1)$"
      },
      "fmcOutsideNic":{
          "type":"string",
          "pattern":"^.*0/(0|1)$"
      },
      "fmcOutsideZone":{
          "type":"string",
          "pattern":"^...*$"
      },
      "fmcInsideZone":{
          "type":"string",
          "pattern":"^...*$"
      },
      "interfaceConfig":{
          "type":"array",
          "items":{
              "type":"object",
              "properties":{
                  "managementOnly": {
                      "type":"string",
                      "pattern":"^...*$"
                  },
                  "MTU":{
                      "type":"string",
                      "pattern":"^[1-9][0-9][0-9][0-9]$"
                  },
                  "securityZone":{
                      "type":"object",
                      "properties":{
                          "name":{
                              "type":"string",
                              "pattern":"^...*$"
                          }
                      },
                      "required":[
                          "name"
                      ]
                  },
                  "mode":{
                      "type":"string",
                      "pattern":"^...*$"
                  },
                  "ifname":{
                      "type":"string",
                      "pattern":"^...*$"
                  },
                  "name":{
                      "type":"string",
                      "pattern":"^.*0/(0|1)$"
                  }
              },
              "required":[
                  "managementOnly",
                  "MTU",
                  "securityZone",
                  "mode",
                  "ifname",
                  "name"
              ]
          }
      },
      "SingleArmTrafficRoutes":{
          "type":"array",
          "items":{
              "type":"object",
              "properties":{
                  "interface":{
                      "type":"string",
                      "pattern":"^...*$"
                  },
                  "network":{
                      "type":"string",
                      "pattern":"^...*$"
                  },
                  "gateway":{
                      "type":"string",
                      "pattern":"(^$|^..*$|^(?:(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9][0-9]|[0-9])(\\.(?!$)|$)){4}$)"
                  },
                  "metric":{
                      "type":"string",
                      "pattern":"^[1-9]\\d*$"
                  }
              },
              "required":[
                  "interface",
                  "network",
                  "gateway",
                  "metric"
              ]
          }
      }
  },
  "required":[
      "licenseCaps",
      "fmcIpforDeviceReg",
      "RegistrationId",
      "NatId",
      "fmcAccessPolicyName",
      "fmcPlatformPolicyName",
      "fmcInsideNicName",
      "fmcOutsideNicName",
      "fmcInsideNic",
      "fmcOutsideNic",
      "fmcInsideZone",
      "fmcOutsideZone",
      "interfaceConfig",
      "SingleArmTrafficRoutes"
  ]
}